{
  "name": "unplugin-jsx-string",
  "version": "0.3.0",
  "packageManager": "pnpm@7.1.9",
  "description": "Converts JSX to HTML strings at compile time.",
  "keywords": [
    "unplugin",
    "jsx",
    "tsx",
    "string",
    "html",
    "convert",
    "react"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "benchmark": "node benchmark/index.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.18.4",
    "@babel/types": "^7.18.4",
    "@rollup/pluginutils": "^4.2.1",
    "entities": "^4.3.0",
    "estree-walker": "^2.0.2",
    "json5": "^2.2.1",
    "magic-string": "^0.26.2",
    "unplugin": "^0.7.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^2.2.2",
    "@types/benchmark": "^2.1.1",
    "@types/node": "*",
    "@types/react": "^18.0.12",
    "benchmark": "^2.1.4",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "eslint-define-config": "^1.5.0",
    "fast-glob": "^3.2.11",
    "prettier": "^2.6.2",
    "tsup": "^6.1.0",
    "tsx": "^3.4.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.14.1"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
